{
  "name": "helene",
  "version": "1.0.0",
  "description": "Real-time Web Apps for Node.js",
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx 'src/**/*'",
    "browser:test": "karma start",
    "browser:test:watch": "karma start --auto-watch --no-single-run",
    "test": "nodemon --watch 'node_modules/**/*' -e ts,tsx --exec 'yarn run test:mocha'",
    "test:mocha": "NODE_ENV=test mocha -r @swc-node/register --file src/test/index.ts --extension js,jsx,ts,tsx --timeout 10000 --exit --exclude='browser/*' 'src/test/node/**/*.test.ts' 'src/react/**/*.test.tsx'"
  },
  "dependencies": {
    "@lukeed/uuid": "^2.0.1",
    "@sanity/eventsource": "^5.0.2",
    "ahooks": "^3.9.0",
    "avl": "^1.6.0",
    "cors": "^2.8.5",
    "ejson2": "^1.1.0",
    "eventemitter2": "^6.4.9",
    "express": "^5.1.0",
    "express-rate-limit": "^7.2.0",
    "http-terminator": "^3.2.0",
    "idb": "^8.0.3",
    "limiter": "^2.1.0",
    "lodash": "^4.17.21",
    "memoizee": "^0.4.15",
    "query-string": "^7.1.3",
    "redis": "^4.6.7",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "use-debounce": "^7.0.1",
    "ws": "^8.18.3",
    "yup": "^1.6.1",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@swc-node/register": "^1.10.10",
    "@swc/core": "^1.12.14",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "chai": "^4.3.10",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "esbuild": "^0.20.2",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^26.1.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-esbuild": "^2.3.0",
    "karma-mocha": "2.0.1",
    "lodash": "^4.17.21",
    "mocha": "11.7.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "puppeteer": "22.6.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sinon": "^21.0.0",
    "sinon-chai": "^4.0.0",
    "supertest": "^6.3.3",
    "typescript": "^5.8.3"
  },
  "author": "Leonardo Venturini",
  "license": "MIT",
