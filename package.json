{
  "name": "helene",
  "version": "0.0.0",
  "description": "A real-time reactive framework for Node and React",
  "browser": "./lib/index.browser.js",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:docs": "typedoc src --out docs",
    "watch": "tsc --watch",
    "clean": "rm -rf lib",
    "test": "nodemon --watch 'src/**/*' -e ts,tsx --exec 'yarn test:mocha'",
    "test:mocha": "NODE_ENV=test mocha -r ts-node/register/transpile-only 'src/**/*.test.*' --extension ts,tsx --file 'src/utils/setup-tests.ts' --exit"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "classnames": "^2.3.1",
    "cors": "^2.8.5",
    "eventemitter2": "^6.4.4",
    "express": "^4.17.1",
    "isomorphic-ws": "^4.0.1",
    "lodash": "^4.17.21",
    "memoizee": "^0.4.14",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "redis": "^3.1.2",
    "secure-ls": "^1.2.6",
    "use-debounce": "^7.0.0",
    "uuid": "^8.3.2",
    "ws": "^8.2.3"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.1.0",
    "@testing-library/react": "^11.2.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^12.5.0",
    "@tstt/eslint-config": "latest",
    "@types/express": "^4.17.13",
    "@types/jsdom": "^16.2.13",
    "@types/jsdom-global": "^3.0.2",
    "@types/mocha": "^9.1.0",
    "@types/react-virtualized": "^9.21.15",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "jsdom": "^18.1.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^9.1.3",
    "sinon": "^11.1.2",
    "sinon-chai": "^3.7.0",
    "ts-node": "^10.1.0",
    "typescript": "^4.4.4",
    "typedoc": "^0.22.7",
    "wtfnode": "^0.9.1"
  },
  "author": "Leonardo Venturini",
  "license": "UNLICENSED",
  "private": true
}
