{
  "name": "helene",
  "version": "1.0.0",
  "description": "Real-time Web Apps for Node.js",
  "type": "commonjs",
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx 'src/**/*'",
    "browser:test": "karma start karma.config.js",
    "browser:test:watch": "karma start karma.config.js --auto-watch --no-single-run",
    "bun:test": "nodemon --watch 'node_modules/**/*' -e ts,tsx --exec 'yarn run bun:test:mocha'",
    "bun:test:mocha": "NODE_ENV=test bun --bun run mocha --extension ts,tsx --file index.ts --exit --exclude='browser/*' '**/*.test.*'",
    "node:test": "nodemon --watch 'node_modules/**/*' -e ts,tsx --exec 'yarn run node:test:mocha'",
    "node:test:mocha": "NODE_ENV=test mocha -r ts-node/register/transpile-only --file src/test/index.ts --extension js,jsx,ts,tsx --timeout 10000 --exit --exclude='browser/*' 'node/**/*.test.ts'"
  },
  "dependencies": {
    "@lukeed/uuid": "^2.0.1",
    "@sanity/eventsource": "^5.0.0",
    "ahooks": "^3.7.7",
    "cors": "^2.8.5",
    "ejson2": "^1.1.0",
    "eventemitter2": "^6.4.7",
    "express": "^5.0.1",
    "express-rate-limit": "^7.2.0",
    "http-terminator": "^3.2.0",
    "limiter": "^2.1.0",
    "lodash": "^4.17.21",
    "memoizee": "^0.4.15",
    "query-string": "^7.1.3",
    "redis": "^4.6.7",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "use-debounce": "^7.0.1",
    "ws": "^8.13.0",
    "yup": "^1.4.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.0",
    "@types/express": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "chai": "^5.2.1",
    "chai-as-promised": "^8.0.1",
    "chai-subset": "^1.6.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.0.3",
    "react": "^19.1.0",
    "sinon": "^21.0.0",
    "sinon-chai": "^4.0.0",
    "typescript": "^5.7.2",
    "@types/chai": "^4.3.6",
    "@types/chai-as-promised": "^7.1.6",
    "@types/chai-subset": "^1.3.3",
    "@types/mocha": "^10.0.2",
    "@types/sinon": "^10.0.18",
    "@types/sinon-chai": "^3.2.10",
    "@types/supertest": "^2.0.14",
    "esbuild": "^0.20.2",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-esbuild": "^2.3.0",
    "karma-mocha": "^2.0.1",
    "lodash": "^4.17.21",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "puppeteer": "^22.6.1",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1"
  },
  "author": "Leonardo Venturini",
  "license": "MIT",
  "repository": "https://github.com/leonardoventurini/helene",
  "keywords": [
    "websockets",
    "rpc",
    "react",
    "reactjs",
    "methods",
    "events",
    "node",
    "nodejs"
  ],
  "bugs": {
    "url": "https://github.com/leonardoventurini/helene/issues"
  },
  "volta": {
    "node": "24.4.1",
    "yarn": "1.22.22"
  },
  "private": true
}
