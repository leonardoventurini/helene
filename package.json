{
  "name": "helene",
  "version": "0.0.5",
  "description": "A real-time reactive framework for Node and React",
  "browser": "lib/index.browser.js",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:docs": "typedoc src --out docs",
    "watch": "tsc --watch",
    "clean": "rm -rf lib",
    "test": "nodemon --watch 'src/**/*' -e ts,tsx --exec 'yarn test:mocha'",
    "test:mocha": "NODE_ENV=test mocha -r ts-node/register/transpile-only 'src/**/*.test.*' --extension ts,tsx --file 'src/utils/setup-tests.ts' --exit"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "classnames": "^2.3.1",
    "cors": "^2.8.5",
    "eventemitter2": "^6.4.5",
    "express": "^4.17.3",
    "isomorphic-ws": "^4.0.1",
    "lodash": "^4.17.21",
    "memoizee": "^0.4.15",
    "query-string": "^7.1.1",
    "react-singleton-hook": "^3.2.3",
    "redis": "^4.0.6",
    "resize-observer-polyfill": "^1.5.1",
    "rxjs": "^7.5.5",
    "secure-ls": "^1.2.6",
    "use-debounce": "^7.0.1",
    "uuid": "^8.3.2",
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.1.0",
    "@testing-library/react": "^11.2.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^12.5.0",
    "@tstt/eslint-config": "latest",
    "@types/chai-subset": "^1.3.3",
    "@types/express": "^4.17.13",
    "@types/jsdom": "^16.2.14",
    "@types/jsdom-global": "^3.0.2",
    "@types/lodash": "^4.14.181",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.23",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "jsdom": "^19.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^9.2.2",
    "nodemon": "^2.0.15",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sinon": "^13.0.1",
    "sinon-chai": "^3.7.0",
    "ts-node": "^10.7.0",
    "typedoc": "^0.22.13",
    "typescript": "^4.6.3",
    "wtfnode": "^0.9.1"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "author": "Leonardo Venturini",
  "license": "MIT",
  "repository": "https://github.com/leonardoventurini/helene",
  "keywords": [
    "websockets",
    "rpc",
    "rxjs",
    "reactjs",
    "methods",
    "events"
  ],
  "bugs": {
    "url": "https://github.com/leonardoventurini/helene/issues"
  }
}
