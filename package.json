{
  "name": "helene",
  "version": "1.2.26",
  "description": "Delightful Real-time Apps for Node.js",
  "browser": "index.browser.js",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "build": "tsc",
    "watch": "tsc --watch",
    "clean": "rm -rf client react server data utils index.* tsconfig.tsbuildinfo",
    "rebuild": "yarn run clean && yarn run build",
    "bun:test": "nodemon --watch 'src/**/*' -e ts,tsx --exec 'bun run bun:test:mocha'",
    "bun:test:mocha": "bun run ./node_modules/mocha/bin/mocha.js 'src/**/*.test.*' --extensions ts,tsx --file 'src/tests/utils/setup-tests.ts' --exit",
    "node:test": "nodemon --watch 'src/**/*' -e ts,tsx --exec 'bun run node:test:mocha'",
    "node:test:mocha": "node --require @swc/register ./node_modules/mocha/bin/mocha.js 'src/**/*.test.*' --extensions ts,tsx --file 'src/tests/utils/setup-tests.ts' --exit"
  },
  "dependencies": {
    "@lukeed/uuid": "^2.0.1",
    "@sanity/eventsource": "^5.0.0",
    "@swc/core": "^1.3.84",
    "@swc/register": "^0.1.10",
    "ahooks": "^3.7.7",
    "classnames": "^2.3.2",
    "cors": "^2.8.5",
    "ejson2": "^1.1.0",
    "eventemitter2": "^6.4.7",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^6.7.0",
    "fetch-undici": "^3.0.1",
    "http-terminator": "^3.2.0",
    "isomorphic-ws": "^5.0.0",
    "limiter": "^2.1.0",
    "lodash": "^4.17.21",
    "memoizee": "^0.4.15",
    "mkdirp": "^2.1.6",
    "query-string": "^7.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "redis": "^4.6.7",
    "use-debounce": "^7.0.1",
    "ws": "^8.13.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.3.1",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/chai-as-promised": "^7.1.5",
    "@types/chai-subset": "^1.3.3",
    "@types/express": "^4.17.13",
    "@types/jsdom": "^20.0.0",
    "@types/jsdom-global": "^3.0.2",
    "@types/lodash": "^4.14.181",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.23",
    "@types/sinon": "^10.0.15",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "bun-types": "^1.0.1",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^21.1.2",
    "jsdom-global": "^3.0.2",
    "mocha": "10.2.0",
    "nodemon": "^2.0.22",
    "prettier": "^3.0.3",
    "sinon": "^15.2.0",
    "sinon-chai": "^3.7.0",
    "supertest": "^6.2.4",
    "typescript": "^5.1.3"
  },
  "author": "Leonardo Venturini",
  "license": "MIT",
  "repository": "https://github.com/leonardoventurini/helene",
  "keywords": [
    "websockets",
    "rpc",
    "reactjs",
    "methods",
    "events"
  ],
  "bugs": {
    "url": "https://github.com/leonardoventurini/helene/issues"
  },
  "volta": {
    "node": "20.6.1",
    "yarn": "1.22.19"
  }
}
