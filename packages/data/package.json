{
  "name": "@helenejs/data",
  "version": "1.0.0",
  "description": "Real-time Web Apps for Node.js and Bun",
  "browser": "index.browser.js",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "build": "tsc",
    "watch": "tsc --watch",
    "clean": "rm -rf client react server data utils index.* tsconfig.tsbuildinfo",
    "rebuild": "yarn run clean && yarn run build",
    "bun:test": "nodemon --watch 'src/**/*' -e ts,tsx --exec 'bun run bun:test:mocha'",
    "bun:test:mocha": "bun run ./node_modules/mocha/bin/mocha.js 'src/**/*.test.*' --extensions ts,tsx --file 'src/tests/utils/setup-tests.ts' --exit",
    "node:test": "nodemon --watch 'src/**/*' -e ts,tsx --exec 'bun run node:test:mocha'",
    "node:test:mocha": "node --require @swc/register ./node_modules/mocha/bin/mocha.js 'src/**/*.test.*' --extensions ts,tsx --file 'src/tests/utils/setup-tests.ts' --exit"
  },
  "dependencies": {
    "@lukeed/uuid": "^2.0.1",
    "@sanity/eventsource": "^5.0.0",
    "@swc/core": "^1.3.84",
    "@swc/register": "^0.1.10",
    "ahooks": "^3.7.7",
    "cors": "^2.8.5",
    "ejson2": "^1.1.0",
    "eventemitter2": "^6.4.7",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^6.7.0",
    "fetch-undici": "^3.0.1",
    "http-terminator": "^3.2.0",
    "isomorphic-ws": "^5.0.0",
    "limiter": "^2.1.0",
    "lodash": "^4.17.21",
    "memoizee": "^0.4.15",
    "mkdirp": "^2.1.6",
    "query-string": "^7.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "redis": "^4.6.7",
    "use-debounce": "^7.0.1",
    "ws": "^8.13.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@helenejs/testing": "workspace:*",
    "@types/lodash": "^4.14.181",
    "typescript": "^5.1.3"
  },
  "author": "Leonardo Venturini",
  "license": "MIT",
  "repository": "https://github.com/leonardoventurini/helene",
  "keywords": [
    "websockets",
    "rpc",
    "react",
    "reactjs",
    "methods",
    "events",
    "node",
    "nodejs",
    "bun"
  ],
  "bugs": {
    "url": "https://github.com/leonardoventurini/helene/issues"
  },
  "volta": {
    "node": "20.6.1",
    "yarn": "1.22.19"
  },
  "private": true
}
